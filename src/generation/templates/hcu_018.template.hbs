	        	<td>{{#if via_aerea.patologia_intubacion.no}}X{{/if}}</td>
	        	<th> NO </th>
	        	<td>X</td>
	        </tr>
	        <tr>
	        	<th class="th4" colspan="6">OTROS</th>
	        	<td class="td1" colspan="16">{{otros}}</td>
	        </tr>
	        <tr>
	        	<th class="th4" colspan="7">TÓRAX</th>
	        	<td class="td1" colspan="16">{{torax}}</td>
	        </tr>
	        <tr>
	        	<th class="th4" colspan="7">CORAZÓN</th>
	        	<td class="td1" colspan="16">{{corazon}}</td>
	        </tr>
	        <tr>
	        	<th class="th4" colspan="7">PULMONES</th>
